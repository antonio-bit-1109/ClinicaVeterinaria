@model ClinicaVeterinaria.Models.Vendite

@{
    ViewData["Title"] = "Details";
}

<h1>Dettagli Vendita</h1>

<div>
    <h4>Vendite</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Cf)
        </dt>
        <dd class="col-sm-10 mb-3">
            @Html.DisplayFor(model => model.Cf)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdProdottoNavigation)
        </dt>
        <dd class="col-sm-10 mb-3">
            @Html.DisplayFor(model => model.IdProdottoNavigation.IdProdotto)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdUtenteNavigation)
        </dt>
        <dd class="col-sm-10 mb-3">
            @Html.DisplayFor(model => model.IdUtenteNavigation.IdUtente)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdricettaMedicaNavigation)
        </dt>
        <dd class="col-sm-10 mb-3">
            @Html.DisplayFor(model => model.IdricettaMedicaNavigation.IdricettaMedica)
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-success me-2" asp-action="Edit" asp-route-id="@Model?.IdVendita">Modifca</a>
    <a class="btn btn-outline-success" asp-action="Index">Lista Vendite</a>
</div>
